\t// alert('selectItem'+index)\r\n\t\t\t\tconsole.log()\r\n\t\t\t\tthis.selectIndex = index\r\n\t\t\t},\r\n\t\t\tdeleteItem:function(){\r\n\t\t\t\tconsole.log('deleteItem')\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style>\r\n\tinput {\r\n\t\tmargin-left: 20px;\r\n\t}\r\n\t\r\n\t.checked{\r\n\t\tbackground-color:antiquewhite;\r\n\t}\r\n\t\r\n</style>\n"],"sourceRoot":"src/views"}]}